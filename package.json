{
  "name": "typescript-nextjs-starter",
  "description": "A TypeScript starter for Next.js that includes all you need to build amazing projects",
  "version": "1.0.0",
  "private": true,
  "author": "Jo√£o Pedro Schmitz <hey@joaopedro.dev> (@jpedroschmitz)",
  "license": "MIT",
  "keywords": [
    "nextjs",
    "starter",
    "typescript"
  ],
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\"",
    "format": "prettier --ignore-path .gitignore \"src/**/*.+(ts|js|tsx)\" --write",
    "postinstall": "husky install",
    "svg:transform": "./node_modules/@svgr/cli/bin/svgr --ignore-existing --typescript -d src/shared/icons src/shared/assets/svg"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "eslint --ignore-path .gitignore --fix",
      "prettier --ignore-path .gitignore --write"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "3.1.1",
    "@next/bundle-analyzer": "13.4.7",
    "@next/font": "13.4.7",
    "@react-three/drei": "9.77.0",
    "@react-three/fiber": "8.13.3",
    "@svgr/cli": "8.0.1",
    "@types/lodash": "4.14.195",
    "@types/react-lottie": "1.2.6",
    "@types/react-scroll": "1.8.7",
    "@types/three": "0.152.1",
    "@types/uuid": "9.0.2",
    "axios": "1.4.0",
    "classnames": "2.3.2",
    "commitizen": "4.3.0",
    "framer-motion": "10.12.16",
    "framer-motion-3d": "6.5.1",
    "lodash": "4.17.21",
    "lottie-react": "2.4.0",
    "next": "13.4.4",
    "next-compose-plugins": "2.2.1",
    "nookies": "2.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.44.3",
    "react-lottie": "1.2.3",
    "react-modal-hook": "3.0.2",
    "react-phone-input-2": "2.15.1",
    "react-phone-number-input": "3.2.23",
    "react-scroll": "1.8.9",
    "react-select": "5.7.3",
    "sass": "1.62.1",
    "serve": "14.2.0",
    "sharp": "0.32.5",
    "swiper": "9.4.1",
    "three": "0.153.0",
    "uuid": "9.0.0",
    "yup": "1.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.6.5",
    "@commitlint/config-conventional": "17.6.5",
    "@types/node": "18.16.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "@typescript-eslint/eslint-plugin": "5.59.8",
    "@typescript-eslint/parser": "5.59.8",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.41.0",
    "eslint-config-next": "13.4.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-import-resolver-typescript": "3.5.5",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-prettier": "4.2.1",
    "husky": "8.0.3",
    "lint-staged": "13.2.2",
    "prettier": "2.8.8",
    "typescript": "5.0.4"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
